								"body": {
									"mode": "raw",
									"raw": "{\n    \"id\": {{$randomInt}},\n    \"name\": \"{{$randomNoun}}\",\n    \"category\": {\n        \"id\": {{$randomInt}},\n        \"name\": \"Dogs\"\n    },\n    \"photoUrls\": [\n        \"{{$randomImageUrl}}\"\n    ],\n    \"tags\": [\n        {\n            \"id\": {{$randomInt}},\n            \"name\": \"{{$randomCatchPhrase}}\"\n        }\n    ],\n    \"status\": \"available\"\n}"
